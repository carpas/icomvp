mkdir -p /home/rafzdv/webapps/icowebapp/run
pid=$(/sbin/pidof /home/rafzdv/webapps/icowebapp/bin/node)
if echo "$pid" | grep -q " "; then
    pid=""
fi
if [ -n "$pid" ]; then
  user=$(ps -p $pid -o user | tail -n 1)
  if [ $user = "rafzdv" ]; then
    kill "$pid"
    rm -f /home/rafzdv/webapps/icowebapp/run/node.pid
  fi
fi
